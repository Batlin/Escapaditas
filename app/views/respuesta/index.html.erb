<h1>Listing respuesta</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Comment</th>
    <th>User_id</th>
    <th></th>
    <th></th>
  </tr>

<% @respuesta.each do |respuestum| %>
  <tr>
    <td><%= respuestum.description %></td>
    <td><%= respuestum.comment_id %></td>
    <td><%= respuestum.user_id %></td>
    <td><%= link_to 'Show', respuestum %></td>
    <td><%= link_to 'Edit', edit_respuestum_path(respuestum) %></td>
    <td><%= link_to 'Destroy', respuestum, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Respuestum', new_respuestum_path %>
